{
  "name": "server",
  "jest": {
    "moduleFileExtensions": [
      "js"
    ],
    "moduleDirectories": [
      "node_modules",
      "src"
    ]
  },
  "version": "1.0.0",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "test": "cross-env NODE_OPTIONS=--experimental-vm-modules npx jest",
    "test:migrate:up": "knex migrate:latest --env test",
    "test:seed:run": "knex seed:run --env test",
    "test:seed:make": "knex seed:make --env test",
    "test:clear_db": "knex migrate:rollback --env test",
    "server": "nodemon src/app.js",
    "migration": "knex migrate:latest",
    "migration:make": "knex migrate:make",
    "seed": "knex seed:run",
    "seed:make": "knex seed:make",
    "clear:dev_db": "knex migrate:rollback"
  },
  "dependencies": {
    "apollo-datasource-rest": "^0.11.0",
    "apollo-server-express": "^2.21.2",
    "apollo-server-testing": "^2.22.2",
    "bcrypt": "^5.0.1",
    "case": "^1.6.3",
    "cors": "^2.8.5",
    "dataloader": "^2.0.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-jwt": "^6.0.0",
    "graphql": "^15.5.0",
    "graphql-scalars": "^1.9.0",
    "graphql-subscriptions": "^1.2.1",
    "jsonwebtoken": "^8.5.1",
    "knex-cleaner": "^1.3.1",
    "lodash": "^4.17.21",
    "pg": "^8.5.1",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@types/jest": "^26.0.22",
    "cross-env": "^7.0.3",
    "easygraphql-tester": "^6.0.1",
    "faker": "^5.5.3",
    "jest": "^26.6.3",
    "knex": "^0.95.2",
    "nodemon": "^2.0.7"
  },
  "author": "",
  "license": "ISC",
  "description": ""
}
